{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "blade-ui-kit/blade-heroicons": "^1.3",
        "blade-ui-kit/blade-icons": "^1.2",
        "diglactic/laravel-breadcrumbs": "^7.1",
        "guzzlehttp/guzzle": "^7.2",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/framework": "^9.2",
        "laravel/horizon": "^5.9",
        "laravel/jetstream": "^2.6",
        "laravel/sanctum": "^2.14.1",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^2.10",
        "owen-it/laravel-auditing": "^13.0",
        "predis/predis": "^1.1",
        "rappasoft/laravel-livewire-tables": "^2.7",
        "spatie/laravel-enum": "^3.0",
        "spatie/laravel-medialibrary": "^10.3",
        "spatie/laravel-permission": "^5.5",
        "yadahan/laravel-authentication-log": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.8",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.1",
        "nunomaduro/phpinsights": "^2.2",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan livewire:publish"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "csfix": [
            "PHP_CS_FIXER_IGNORE_ENV=1 vendor/bin/php-cs-fixer fix -n",
            "git add .",
            "git commit -m 'ðŸŽ¨ Apply PHP CS Fixer'"
        ],
        "test": [
            "@php vendor/bin/pest --stop-on-error --stop-on-fail "
        ],
        "analyse": [
            "@php vendor/bin/phpstan analyse -c phpstan.dist --error-format=table > storage/logs/phpstan.log"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
